<urlLists>
    <require>
        <define>parseURLReq</define>
        <path>process.mainModule.exports["corePath"] + '/src/serverroot/common/parseURLRequire'</path>
    </require>
    <require>
        <define>timeout</define>
        <path>request-timeout</path>
    </require>
    <require>
        <define>smapi</define>
        <path>../api/sm.api</path>
    </require>
    <item>
        <url>/sm/objects/:name</url>
        <method>get</method>
        <feature>smConfigure</feature>
        <callback>smapi.getObjects</callback>
    </item>
    <item>
        <url>/sm/objects/details/:name</url>
        <method>get</method>
        <feature>smConfigure</feature>
        <callback>smapi.getObjectsDetails</callback>
    </item>
</urlLists>
