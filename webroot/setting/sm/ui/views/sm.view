<script type="text/x-handlebars-template" id="sm-template">
    <div id="{{name}}-container" class="row-fluid ">
        <div id="{{name}}-results" class="row-fluid"></div>
    </div>
</script>

<script type="text/x-handlebars-template" id="sm-edit-layout-template">
    <form id="sm-{{prefixId}}-edit-form" onsubmit="return false;" class="">
        <div id="sm-{{prefixId}}-accordion">
            {{#each groups}}
                <h3>{{this.title}}</h3>
                <div id="sm-{{prefixId}}-{{this.title}}">
                    {{#each this.rows}}
                        <div class="row-fluid margin-0-0-5">
                            {{#each this.elements}}
                                <div id="{{this.id}}" class="{{this.class}}"></div>
                            {{/each}}
                        </div>
                    {{/each}}
                </div>
            {{/each}}
        </div>
    </form>
</script>

<script type="text/x-handlebars-template" id="sm-form-input-template">
    <label>{{label}}</label>
    <div class="row-fluid">
        <input id="{{id}}" name="{{name}}" class="{{class}}" value="{{value}}"/>
    </div>
</script>

<script type="text/x-handlebars-template" id="sm-actions-template">
    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
        <i class="icon-cog"></i>
    </a>
    <ul class="pull-right dropdown-menu dropdown-caret">
        {{#each actions}}
            <li>
                <a onclick="">
                    <i class="{{this.iconClass}}"></i>  &nbsp; {{this.label}}
                </a>
            </li>
        {{/each}}
    </ul>
</script>

<script type="text/x-handlebars-template" id="sm-tags-template">
    {{#each this}}
        <a href="/#p=setting_sm_servers&q[tag][{{@key}}]={{this}}"><span class="badge badge-color-{{@index}}">{{this}}</span></a>
    {{/each}}
</script>


